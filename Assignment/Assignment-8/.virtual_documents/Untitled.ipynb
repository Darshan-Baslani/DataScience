data = {'fruits': ['mango', 'banana', 'apple', 'kiwi', 'watermelon'],
        'quantity': [40, 30, 10, 20, 25]}
fruits = pd.DataFrame(data)

def calculate_sum(fruits):
    values = fruits['quantity']
    ans = sum(values[:3])
    print(ans)

calculate_sum(fruits)






